{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Usuario",
  "type": "object",
  "properties": {
    "nombre": {
      "type": "string",
      "minLength": 1
    },
    "correo": {
      "type": "string",
      "format": "email"
    },
    "fechaAlta": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["nombre", "correo", "fechaAlta"]
}